<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/md3_surface_container_high_dark"/>
            <corners android:radius="12dp"/>
            <stroke android:width="1dp" android:color="@color/md3_primary_dark"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/md3_surface_container_dark"/>
            <corners android:radius="12dp"/>
            <stroke android:width="2dp" android:color="@color/md3_primary_dark"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/md3_surface_variant_dark"/>
            <corners android:radius="12dp"/>
            <stroke android:width="1dp" android:color="@color/md3_outline_variant_dark"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/md3_surface_container_dark"/>
            <corners android:radius="12dp"/>
            <stroke android:width="1dp" android:color="@color/md3_outline_dark"/>
        </shape>
    </item>
</selector>